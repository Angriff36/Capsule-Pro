{
  "name": "@repo/manifest",
  "version": "0.3.0",
  "dependencies": {},
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.10.5",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    },
    "./compiler": {
      "types": "./dist/compiler.d.ts",
      "import": "./dist/compiler.js"
    },
    "./generator": {
      "types": "./src/manifest/generator.ts",
      "import": "./src/manifest/generator.ts"
    },
    "./parser": {
      "types": "./src/manifest/parser.ts",
      "import": "./src/manifest/parser.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./stores.node": {
      "types": "./src/manifest/stores.node.ts",
      "import": "./src/manifest/stores.node.ts"
    }
  },
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "bin": {
    "manifest-compile": "./bin/compile.ts",
    "capsule-pro-generate": "./bin/capsule-pro-generate.ts"
  },
  "type": "module",
  "types": "./dist/index.d.ts"
}
