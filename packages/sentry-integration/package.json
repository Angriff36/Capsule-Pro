{
  "name": "@repo/sentry-integration",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./keys": {
      "types": "./dist/keys.d.ts",
      "import": "./dist/keys.js"
    },
    "./webhook": {
      "types": "./dist/webhook.d.ts",
      "import": "./dist/webhook.js"
    },
    "./queue": {
      "types": "./dist/queue.d.ts",
      "import": "./dist/queue.js"
    },
    "./runner": {
      "types": "./dist/runner.d.ts",
      "import": "./dist/runner.js"
    },
    "./slack": {
      "types": "./dist/slack.d.ts",
      "import": "./dist/slack.js"
    },
    "./prisma-store": {
      "types": "./dist/prisma-store.d.ts",
      "import": "./dist/prisma-store.js"
    },
    "./fixer": {
      "types": "./dist/fixer.d.ts",
      "import": "./dist/fixer.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/keys.ts src/webhook.ts src/queue.ts src/runner.ts src/slack.ts src/prisma-store.ts src/fixer.ts --format esm --dts",
    "dev": "tsup src/index.ts src/keys.ts src/webhook.ts src/queue.ts src/runner.ts src/slack.ts src/prisma-store.ts src/fixer.ts --format esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.25",
    "@repo/database": "workspace:*",
    "@repo/observability": "workspace:*",
    "@slack/web-api": "^7.0.0",
    "@t3-oss/env-nextjs": "^0.10.0",
    "ai": "^6.0.70",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
